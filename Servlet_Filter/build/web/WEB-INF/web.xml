<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <filter>
        <filter-name>Servlet_Filter</filter-name>
        <filter-class>controller.authentication.Servlet_Filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Servlet_Filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>controller.authentication</servlet-name>
        <servlet-class>controller.authentication.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>controller.authentication</servlet-name>
        <url-pattern>/login_auth</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>InsertController</servlet-name>
        <servlet-class>controller.InsertController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListController</servlet-name>
        <servlet-class>controller.ListController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertController</servlet-name>
        <url-pattern>/auth/dosmth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListController</servlet-name>
        <url-pattern>/auth/test</url-pattern>
    </servlet-mapping>
</web-app>
